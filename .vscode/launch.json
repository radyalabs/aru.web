    "version": "0.2.0",
    "configurations": [
        {
            "name": "Job",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build-job",
            "program": "${workspaceFolder}/Trisatech.MWorkforce.Job/bin/Debug/net6.0/Trisatech.MWorkforce.Job.dll",
            "args": [],
            "cwd": "${workspaceFolder}/Trisatech.MWorkforce.Job",
            "stopAtEntry": false,
            "serverReadyAction": {
                "action": "openExternally",
                "pattern": "\\bNow listening on:\\s+(https?://\\S+)"
            },
            "env": {
                "ASPNETCORE_ENVIRONMENT": "Development"
            },
            "sourceFileMap": {
                "/Views": "${workspaceFolder}/Views"
            }
        },
        {
            "name": "Api",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build-api",
            "program": "${workspaceFolder}/Trisatech.MWorkforce.Api/bin/Debug/net6.0/Trisatech.MWorkforce.Api.dll",
            "args": [],
            "cwd": "${workspaceFolder}/Trisatech.MWorkforce.Api",
            "stopAtEntry": false,
            "serverReadyAction": {
                "action": "openExternally",
                "pattern": "\\bNow listening on:\\s+(https?://\\S+)"
            },
            "env": {
                "ASPNETCORE_ENVIRONMENT": "Development"
            },
            "sourceFileMap": {
                "/Views": "${workspaceFolder}/Views"
            }
        },
        {
            "name": "Cms",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build-cms",
            "program": "${workspaceFolder}/Trisatech.MWorkforce.Cms/bin/Debug/net6.0/Trisatech.MWorkforce.Cms.dll",
            "args": [],
            "cwd": "${workspaceFolder}/Trisatech.MWorkforce.Cms",
            "stopAtEntry": false,
            "serverReadyAction": {
                "action": "openExternally",
                "pattern": "\\bNow listening on:\\s+(https?://\\S+)"
            },
            "env": {
                "ASPNETCORE_ENVIRONMENT": "Development"
            },
            "sourceFileMap": {
                "/Views": "${workspaceFolder}/Views"
            }
        },
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach"
        }
    ]
